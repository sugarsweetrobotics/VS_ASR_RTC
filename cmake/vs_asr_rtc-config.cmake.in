# VS_ASR_RTC CMake config file
#
# This file sets the following variables:
# VS_ASR_RTC_FOUND - Always TRUE.
# VS_ASR_RTC_INCLUDE_DIRS - Directories containing the VS_ASR_RTC include files.
# VS_ASR_RTC_IDL_DIRS - Directories containing the VS_ASR_RTC IDL files.
# VS_ASR_RTC_LIBRARIES - Libraries needed to use VS_ASR_RTC.
# VS_ASR_RTC_DEFINITIONS - Compiler flags for VS_ASR_RTC.
# VS_ASR_RTC_VERSION - The version of VS_ASR_RTC found.
# VS_ASR_RTC_VERSION_MAJOR - The major version of VS_ASR_RTC found.
# VS_ASR_RTC_VERSION_MINOR - The minor version of VS_ASR_RTC found.
# VS_ASR_RTC_VERSION_REVISION - The revision version of VS_ASR_RTC found.
# VS_ASR_RTC_VERSION_CANDIDATE - The candidate version of VS_ASR_RTC found.

message(STATUS "Found VS_ASR_RTC-@PROJECT_VERSION@")
set(VS_ASR_RTC_FOUND TRUE)

find_package(<dependency> REQUIRED)

#set(VS_ASR_RTC_INCLUDE_DIRS
#    "@CMAKE_INSTALL_PREFIX@/include/@PROJECT_NAME_LOWER@-@PROJECT_VERSION_MAJOR@"
#    ${<dependency>_INCLUDE_DIRS}
#    )
#
#set(VS_ASR_RTC_IDL_DIRS
#    "@CMAKE_INSTALL_PREFIX@/include/@PROJECT_NAME_LOWER@-@PROJECT_VERSION_MAJOR@/idl")
set(VS_ASR_RTC_INCLUDE_DIRS
    "@CMAKE_INSTALL_PREFIX@/include/@CPACK_PACKAGE_FILE_NAME@"
    ${<dependency>_INCLUDE_DIRS}
    )
set(VS_ASR_RTC_IDL_DIRS
    "@CMAKE_INSTALL_PREFIX@/include/@CPACK_PACKAGE_FILE_NAME@/idl")


if(WIN32)
    set(VS_ASR_RTC_LIBRARIES
        "@CMAKE_INSTALL_PREFIX@/@LIB_INSTALL_DIR@/@CMAKE_SHARED_LIBRARY_PREFIX@@PROJECT_NAME_LOWER@@CMAKE_STATIC_LIBRARY_SUFFIX@"
        ${<dependency>_LIBRARIES}
        )
else(WIN32)
    set(VS_ASR_RTC_LIBRARIES
        "@CMAKE_INSTALL_PREFIX@/@LIB_INSTALL_DIR@/@CMAKE_SHARED_LIBRARY_PREFIX@@PROJECT_NAME_LOWER@@CMAKE_SHARED_LIBRARY_SUFFIX@"
        ${<dependency>_LIBRARIES}
        )
endif(WIN32)

set(VS_ASR_RTC_DEFINITIONS ${<dependency>_DEFINITIONS})

set(VS_ASR_RTC_VERSION @PROJECT_VERSION@)
set(VS_ASR_RTC_VERSION_MAJOR @PROJECT_VERSION_MAJOR@)
set(VS_ASR_RTC_VERSION_MINOR @PROJECT_VERSION_MINOR@)
set(VS_ASR_RTC_VERSION_REVISION @PROJECT_VERSION_REVISION@)
set(VS_ASR_RTC_VERSION_CANDIDATE @PROJECT_VERSION_CANDIDATE@)

